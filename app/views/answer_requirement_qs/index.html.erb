<h1>Listing answer_requirement_qs</h1>

<table>
  <thead>
    <tr>
      <th>Answer1</th>
      <th>Answer2</th>
      <th>Answer3</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @answer_requirement_qs.each do |answer_requirement_q| %>
      <tr>
        <td><%= answer_requirement_q.answer1 %></td>
        <td><%= answer_requirement_q.answer2 %></td>
        <td><%= answer_requirement_q.answer3 %></td>
        <td><%= link_to 'Show', answer_requirement_q %></td>
        <td><%= link_to 'Edit', edit_answer_requirement_q_path(answer_requirement_q) %></td>
        <td><%= link_to 'Destroy', answer_requirement_q, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Answer requirement q', new_answer_requirement_q_path %>
